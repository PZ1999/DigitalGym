          <Tab text="Intro">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="AddClassIntro" layoutY="70.0" prefHeight="272.0" prefWidth="309.0" />
                        <TextField fx:id="AddClassIntroUrl" layoutX="319.0" layoutY="83.0" prefHeight="224.0" prefWidth="266.0" />
